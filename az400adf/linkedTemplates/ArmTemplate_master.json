{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "az400adf"
		},
		"AzureProductable_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureProductable'"
		},
		"AzureSqlDatabaseNorthProd_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlDatabaseNorthProd'"
		},
		"advWorkBlob_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'advWorkBlob'"
		},
		"AzureSqlDatabaseNortwind_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlDatabaseNortwind'"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "az400adf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureProductable_connectionString": {
						"value": "[parameters('AzureProductable_connectionString')]"
					},
					"AzureSqlDatabaseNorthProd_connectionString": {
						"value": "[parameters('AzureSqlDatabaseNorthProd_connectionString')]"
					},
					"advWorkBlob_connectionString": {
						"value": "[parameters('advWorkBlob_connectionString')]"
					},
					"AzureSqlDatabaseNortwind_connectionString": {
						"value": "[parameters('AzureSqlDatabaseNortwind_connectionString')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}